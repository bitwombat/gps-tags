[Unit]
Description=Dog tag tracking
After=network.target

[Service]
ExecStart=/root/main
Restart=always
User=root
Group=root
Environment=MONGO_URL="mongodb://127.0.0.1:27017"
WorkingDirectory=/root
#StandardOutput=file:/root/dog-tags_stdout.log
#StandardError=file:/root/dog-tags_stderr.log

[Install]
WantedBy=multi-user.target
